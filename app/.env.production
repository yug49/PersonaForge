# Production Environment Variables for PersonaForge
# These are the environment variables used in production builds

# Build optimization
GENERATE_SOURCEMAP=false
REACT_APP_NODE_ENV=production

# Performance optimizations
INLINE_RUNTIME_CHUNK=false

# Security headers (handled by Vercel)
REACT_APP_ENABLE_SECURITY_HEADERS=true

